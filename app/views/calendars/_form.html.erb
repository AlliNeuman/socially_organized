<h1>Create a New Calendar</h1>

<%= form_for calendar do |f| %>
<div class="card row">
  <div class="row">
      <%= f.label :name, 'Calendar Name' %>
    </div>

<div class="row">
<%= f.text_field :name %>
</div>

<div class="row buttons">
<%= f.submit class: "btn btn-primary center"%>
</div>
<% end %>
</div>
